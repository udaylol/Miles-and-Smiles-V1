{
  "name": "backend",
  "version": "0.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "worker": "node worker.js",
    "worker:dev": "nodemon worker.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run worker:dev\"",
    "seed": "node seedGames.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js test/unit --runInBand",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js test/integration --runInBand",
    "test:verbose": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose"
  },
  "keywords": [],
  "author": "Uday Dhir",
  "license": "ISC",
  "description": "Backend server for Miles and Smiles",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^7.0.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "redis": "^5.9.0",
    "socket.io": "^4.8.1",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.1.2",
    "mongodb-memory-server-core": "^11.0.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4"
  }
}
